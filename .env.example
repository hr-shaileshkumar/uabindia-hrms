# Example environment variables for local development
# Copy this file to `.env` and replace values with secrets

# JWT settings (required)
Jwt__Key=REPLACE_WITH_STRONG_SECRET
Jwt__Issuer=UabIndia
Jwt__Audience=UabIndiaUsers

# Database connection (example for local SQL Server)
ConnectionStrings__DefaultConnection=Server=localhost;Database=UabIndiaDb;User Id=sa;Password=YourStrong!Passw0rd;TrustServerCertificate=True;

# ASP.NET environment
ASPNETCORE_ENVIRONMENT=Development

# Optional: port binding for the helper runner / Docker
PORT=5000
