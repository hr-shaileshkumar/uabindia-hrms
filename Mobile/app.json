{
  "expo": {
    "name": "UABIndia HRMS",
    "slug": "uabindiahrms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#16a34a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uabindiahrms.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#16a34a"
      },
      "enableProguardInReleaseBuilds": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.uabindiahrms.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow UABIndia HRMS to access your camera to capture animal images."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow UABIndia HRMS to access your photos to select animal images."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7b95cb3b-0da3-43f2-ba91-a0decf8b0e24"
      }
    }
  }
}
